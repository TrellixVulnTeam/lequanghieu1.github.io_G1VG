{"remainingRequest":"/home/hieule/lequanghieu1.githgub.io/node_modules/vue-loader/lib/index.js??vue-loader-options!/home/hieule/lequanghieu1.githgub.io/src/components/VeeHome.vue?vue&type=script&lang=js&","dependencies":[{"path":"/home/hieule/lequanghieu1.githgub.io/src/components/VeeHome.vue","mtime":1623600283671},{"path":"/home/hieule/lequanghieu1.githgub.io/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/home/hieule/lequanghieu1.githgub.io/node_modules/babel-loader/lib/index.js","mtime":315532800000},{"path":"/home/hieule/lequanghieu1.githgub.io/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/home/hieule/lequanghieu1.githgub.io/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["VeeHome.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsDA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"VeeHome.vue","sourceRoot":"src/components","sourcesContent":["<template>\n  <v-app>\n    <v-content>\n      <div class=\"top vee\"></div>\n      <div class=\"main\">\n        <v-content>\n          <v-card width=\"500\" class=\"mx-auto mt-9\">\n            <v-card-title>Form đăng nhập (vee_validate)</v-card-title>\n            <v-card-text>\n              <validation-observer ref=\"observer\">\n                <form @submit.prevent=\"submit\">\n                  <validation-provider\n                    v-slot=\"{ errors }\"\n                    name=\"email\"\n                    rules=\"required|email\"\n                  >\n                    <v-text-field\n                      v-model=\"name\"\n                      data-vv-delay=\"1000\"\n                      :error-messages=\"errors\"\n                      label=\"Email\"\n                      required\n                    ></v-text-field>\n                  </validation-provider>\n                  <validation-provider\n                    v-slot=\"{ errors }\"\n                    name=\"pass\"\n                    rules=\"required|min:3\"\n                  >\n                    <v-text-field\n                      data-vv-delay=\"1000\"\n                      v-model=\"pass\"\n                      :error-messages=\"errors\"\n                      label=\"password\"\n                      required\n                    ></v-text-field>\n                  </validation-provider>\n                </form>\n              </validation-observer>\n            </v-card-text>\n            <v-card-actions>\n              <v-btn color=\"success\">Register</v-btn>\n              <v-btn color=\"info\" @click=\"submit\">Login</v-btn>\n            </v-card-actions>\n          </v-card>\n        </v-content>\n        <spinner v-if=\"this.$store.state.users.is_login\" />\n      </div>\n      <div class=\"bot vee\"></div>\n    </v-content>\n  </v-app>\n</template>\n\n<script>\nimport spinner from \"./spinner\";\nimport { required, email, min } from \"vee-validate/dist/rules\";\nimport {\n  extend,\n  ValidationObserver,\n  ValidationProvider,\n  setInteractionMode,\n} from \"vee-validate\";\nsetInteractionMode(\"eager\");\n\nextend(\"required\", {\n  ...required,\n  message: \"{_field_} can not be empty đ\",\n});\n\nextend(\"min\", {\n  ...min,\n  message: \"{_field_} may not be little than {length} characters\",\n});\n\nextend(\"email\", {\n  ...email,\n  message: \"Email must be valid\",\n});\n\nexport default {\n  methods: {\n    submit() {\n      this.$store.dispatch(\"usernameLogin\", {\n        email: this.name,\n        pass: this.pass,\n      });\n      if (this.$store.state.users.is_login === true) {\n        setTimeout(() => {\n          this.$router.push(\"/user\");\n        }, 1000);\n      }\n    },\n  },\n  data() {\n    return {\n      name: \"\",\n      pass: \"\",\n    };\n  },\n  components: {\n    ValidationProvider,\n    ValidationObserver,\n    spinner,\n  },\n};\n</script>\n\n<style lang='scss' src='../style.scss' scoped/>\n"]}]}