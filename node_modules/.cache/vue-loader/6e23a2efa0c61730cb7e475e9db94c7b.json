);\n\nexport default {\n  data() {\n    return {\n      name: \"\",\n    };\n  },\n  validations: {\n    name: {\n      required,\n      minLength: minLength(4),\n      maxLength: maxLength(15),\n    },\n  },\n  methods: {\n    delayTouch($v) {\n      $v.$reset();\n      if (touchMap.has($v)) {\n        clearTimeout(touchMap.get($v));\n      }\n      touchMap.set($v, setTimeout($v.$touch, 1000));\n    },\n  },\n};\n</script>\n"]}]}