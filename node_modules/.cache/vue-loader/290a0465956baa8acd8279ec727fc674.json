it()\">Đăng Nhập</button>\n\n    <div class=\"bot vuetify\"></div>\n  </div>\n</template>\n\n<script>\nexport default {\n  methods: {\n    submit() {\n      if (!this.pass || !this.name) {\n        this.warn = true;\n      } else {\n        const account = this.acc.filter(\n          (element) =>\n            element.username === this.name && element.pass === this.pass\n        );\n        if (account.length !== 0) {\n          this.fail = false;\n          this.acc_present = account[0];\n          setTimeout(() => {\n            this.$router.push(\"/user\");\n            localStorage.setItem(\"dataUser\", JSON.stringify(this.acc_present));\n          }, 1000);\n        } else {\n          this.fail = true;\n        }\n      }\n    },\n    nowarn() {\n      this.warn = false;\n    },\n  },\n  data() {\n    return {\n      name: \"\",\n      pass: \"\",\n      fail: false,\n      warn: false,\n      acc_present: \"\",\n      acc: [\n        {\n          id: 1,\n          username: \"user1\",\n          pass: \"123\",\n          fullname: \"Nguyễn Văn A\",\n          birthday: \" 08 03 2001\",\n          email: \"a@gmail.com\",\n          role: \"Admin\",\n        },\n        {\n          id: 2,\n          username: \"user2\",\n          pass: \"123\",\n          fullname: \"Nguyễn Văn B\",\n          birthday: \"Mar 04 2000\",\n          email: \"b@gmail.com\",\n          role: \"User\",\n        },\n      ],\n    };\n  },\n};\n</script>\n\n<style lang='scss' src='../style.scss' scoped/>\n"]}]}