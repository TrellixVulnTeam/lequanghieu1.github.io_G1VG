()\" v-if=\"this.login === 0\">\n        Đăng Nhập\n      </button> -->\n      <spinner class=\"spinner\"  />\n    </div>\n    <div class=\"bot vee\"></div>\n  </div>\n</template>\n\n<script>\nimport spinner from \"./spinner\";\nimport { ValidationProvider } from \"vee-validate\";\nexport default {\n  methods: {\n    submit() {\n      this.login = 1;\n      const account = this.acc.filter(\n        (element) => element.email === this.name && element.pass === this.pass\n      );\n      if (account.length !== 0) {\n        this.fail = false;\n        this.acc_present = account[0];\n        setTimeout(() => {\n          this.$router.push(\"/user\");\n          localStorage.setItem(\"dataUser\", JSON.stringify(this.acc_present));\n        }, 1000);\n      } else {\n        this.login = 0;\n        alert(\"Sai tên đăng nhập hoặc mật khẩu\");\n      }\n    },\n  },\n  data() {\n    return {\n      name: \"\",\n      pass: \"\",\n      login: 0,\n      acc_present: \"\",\n      acc: [\n        {\n          id: 1,\n          username: \"user1\",\n          pass: \"123\",\n          fullname: \"Nguyễn Văn A\",\n          birthday: \" 08 03 2001\",\n          email: \"a@gmail.com\",\n          role: \"Admin\",\n        },\n        {\n          id: 2,\n          username: \"user2\",\n          pass: \"123\",\n          fullname: \"Nguyễn Văn B\",\n          birthday: \"Mar 04 2000\",\n          email: \"b@gmail.com\",\n          role: \"User\",\n        },\n      ],\n    };\n  },\n  components: {\n    ValidationProvider,\n    spinner,\n  },\n};\n</script>\n\n<style lang='scss' src='../style.scss' scoped/>\n"]}]}