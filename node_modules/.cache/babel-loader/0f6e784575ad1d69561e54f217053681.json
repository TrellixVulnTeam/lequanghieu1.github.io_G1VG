AAA,IAAA,EAAA;AAXA,KAAA;AAaA;AAnCA,CAAA","sourcesContent":["<template>\n  <div>\n    <div class=\"top vuetify\"></div>\n    <div class=\"main\">\n      Form đăng nhập (vuetify_validate)\n      <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\n        <v-text-field\n          v-model=\"name\"\n          :counter=\"4\"\n          :rules=\"nameRules\"\n          label=\"Username\"\n          required\n        ></v-text-field>\n\n        <v-text-field\n          v-model=\"pass\"\n          type=\"password\"\n          :rules=\"passRules\"\n          label=\"Password\"\n          required\n        ></v-text-field>\n        <b-button\n          :disabled=\"!valid\"\n          variant=\"success\"\n          class=\"mr-4 vuetify\"\n          @click=\"validate\"\n          v-if=\"!this.$store.state.is_login\"\n        >\n          Đăng nhập\n        </b-button>\n        <spinner class=\"spinner\" v-if=\"this.$store.state.is_login\" />\n      </v-form>\n    </div>\n\n    <div class=\"bot vuetify\"></div>\n  </div>\n</template>\n\n<script>\nimport spinner from \"./spinner\";\nexport default {\n  components: { spinner },\n  methods: {\n    submit() {\n      this.$store.dispatch(\"usernameLogin\", {\n        name: this.name,\n        pass: this.pass,\n      });\n      if (this.$store.state.is_login === true) {\n        setTimeout(() => {\n          this.$router.push(\"/user\");\n        }, 1000);\n      }\n    },\n    validate() {\n      var promise = new Promise((resolve) => {\n        resolve();\n      });\n      promise.then(this.$refs.form.validate());\n    },\n  },\n  data() {\n    return {\n      valid: true,\n      nameRules: [\n        (v) => !!v || \"Name is required\",\n        (v) => (v && v.length >= 4) || \"username must be at least 4 characters\",\n      ],\n      passRules: [\n        (v) => !!v || \"Rules is required\",\n        (v) => (v && v.length >= 3) || \"password must be at least 3 characters\",\n      ],\n      name: \"\",\n      pass: \"\",\n    };\n  },\n};\n</script>\n\n<style lang='scss' src='../style.scss' scoped/>\n"],"sourceRoot":"src/components"}]}