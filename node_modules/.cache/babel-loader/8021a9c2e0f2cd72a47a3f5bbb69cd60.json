urn {\n      name: \"\",\n      pass: \"\",\n      login: 0,\n      acc_present: \"\",\n      acc: [\n        {\n          id: 1,\n          username: \"user1\",\n          pass: \"123\",\n          fullname: \"Nguyễn Văn A\",\n          birthday: \" 08 03 2001\",\n          email: \"a@gmail.com\",\n          role: \"Admin\",\n        },\n        {\n          id: 2,\n          username: \"user2\",\n          pass: \"123\",\n          fullname: \"Nguyễn Văn B\",\n          birthday: \"Mar 04 2000\",\n          email: \"b@gmail.com\",\n          role: \"User\",\n        },\n      ],\n    };\n  },\n  components: {\n    ValidationProvider,\n    spinner,\n  },\n};\n</script>\n\n<style lang='scss' src='../style.scss' scoped/>\n"],"sourceRoot":"src/components"}]}